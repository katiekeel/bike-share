<h1>Station Index</h1>

<a href="/">Home</a>

<% @stations.each do |station| %>
  <p><h3><a href="#"><%= station.name %></a></h3></p>
  <p><h4>Dock Count</h4><br/>
    <%= station.dock_count %></p>
  <p><h4>City</h4><br/>
    <%= station.city_id %></p>
  <p><h4>installation_date</h4><br/>
    <%= station.installation_date %></p>
<% end %>
